    public class character{
    private int atk;//角色的攻击力
    private int hp;//角色的血量
    private int x;//角色的位置坐标x
    private int y;//角色的位置坐标y
    private int speed;//角色的移动速度
    private int exp;   //角色的经验值
 
    public void setY(int y) {
        this.y = y;
    }

    public void setX(int x) {
        this.x = x;
    }

    public void setSpeed(int speed) {
        this.speed = speed;
    }

    public void setHp(int hp) {
        this.hp = hp;
    }

    public void setExp(int exp) {
        this.exp = exp;
    }

    public void setAtk(int atk) {
        this.atk = atk;
    }

    public int getY() {
        return y;
    }

    public int getX() {
        return x;
    }
    public character() {}
    

    public int getSpeed() {
        return speed;
    }

    public int getHp() {
        return hp;
    }

    public int getExp() {
        return exp;
    }

    public int getAtk() {
        return atk;
    }
}
